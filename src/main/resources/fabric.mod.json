{
	"schemaVersion": 1,
	"id": "boredatschool",
	"version": "${version}",
	"name": "BoredAtSchool",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/boredatschool/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "com.boredom.BoredAtSchool",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "com.boredom.BoredAtSchoolClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "com.boredom.BoredAtSchoolDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"boredatschool.mixins.json",
		{
			"config": "boredatschool.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.3",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}